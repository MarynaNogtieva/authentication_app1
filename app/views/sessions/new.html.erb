<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<h1> Login </h1>

<% form_with url: :login_path, local: true do |form| %>
  <div>
    <%= form.label :username %>
    <%= form.text_field :username %>
  </div>
  <div>
     <%= form.label :password %>
     <%= form.text_field :password %>
  </div>
  <%= form.submit %>
<% end %>

